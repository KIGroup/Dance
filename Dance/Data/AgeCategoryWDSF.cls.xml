<Method name="Import">
<ClassMethod>1</ClassMethod>
<ReturnType>%Status</ReturnType>
<Implementation><![CDATA[
	set st = $$$OK
	
	try{
		set groups = ##class(Dance.Wdsf.API).GetAgeGroups()
		
		for i=1:1:groups.Count(){
			set group = groups.GetAt(i)
			
			if (..CodeIdxExists(group.name) = $$$YES){
				set ageCategory = ..CodeIdxOpen(group.name)
			}
			else{
				set ageCategory = ..%New()
				set ageCategory.Name = ##class(Dance.Data.Localization.LText).Create(group.name)
				set ageCategory.Code = group.name
			}
			
			set ageCategory.MaxAge = group.toAge
			set ageCategory.MinAge = group.fromAge
			set ageCategory.MaxDOB = $ZDH($P(group.maxBirthdate,"T",1),3)
			set ageCategory.MinDOB = $ZDH($P(group.minBirthdate,"T",1),3)
			
			w !,ageCategory.%Save()	 
		}
	}
	catch(ex){
		set st = ex.AsStatus()
	}
	
	q st
]]></Implementation>
</Method>

