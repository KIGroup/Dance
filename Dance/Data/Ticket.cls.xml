<FormalSpec><![CDATA[data:%RegisteredObject,&st]]></FormalSpec>
<ReturnType>Ticket</ReturnType>
<Implementation><![CDATA[
	set obj = ..IdInternalIdxOpen(data.idInternal,,.st)
	if $$$ISERR(st){
		set obj = ..%New()
	}
	
	set obj.IdInternal = data.idInternal
	set obj.TStatus = data.tStatus
	set obj.Amount = data.amount
	set obj.PaymentCreated = data.paymentCreated
	set obj.Description = data.description
	set obj.CodeOne = data.codeOne
	set obj.CodeTwo = data.codeTwo
	
	set obj.Participant = ##class(Dance.Data.Participant).%OpenId(data.participantId)
	
	set st = obj.%Save()
	
	quit obj
]]></Implementation>
